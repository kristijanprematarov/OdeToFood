@model Restaurant

<div class="container">
    <table class="table table-bordered">
        <tr>
            <th>@Html.DisplayNameFor(x => x.Name)</th>
            <th>@Html.DisplayNameFor(x => x.Cuisine)</th>
        </tr>
        <tr>
            <td>@Model.Name</td>
            <td>@Model.Cuisine</td>
        </tr>
    </table>
    <a class="btn btn-primary" asp-controller="Home" asp-action="Index">Home</a>
</div>

